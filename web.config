<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="PythonHandler" path="*.py" verb="*" modules="CgiModule" scriptProcessor="C:\Python34\python.exe %s %s" resourceType="File" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
